{
  "name": "monkey-admin",
  "version": "0.2.0",
  "homepage": "https://monkey-admin.surge.sh/",
  "scripts": {
    "coverage": "npm test -- --coverage",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "precommit": "lint-staged",
    "prepush": "CI=true npm test",
    "start": "react-app-rewired start",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andriijas/monkey-admin.git"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.6.3",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "prettier": "^1.10.2",
    "react-app-rewire-less-modules": "^1.2.0",
    "react-app-rewire-vendor-splitting": "^1.3.0",
    "react-app-rewired": "^1.4.1",
    "react-styleguidist": "^6.2.2",
    "react-test-renderer": "^16.3.0-alpha.0",
    "webpack-bundle-analyzer": "^2.10.0"
  },
  "dependencies": {
    "antd": "^3.1.6",
    "prop-types": "^15.6.0",
    "rc-drawer-menu": "^0.5.4",
    "react": "^16.3.0-alpha.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.3.0-alpha.0",
    "react-loadable": "^5.3.1",
    "react-refetch-pre": "^2.0.0-0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "2.0.0-next.9754a231",
    "react-side-effect": "^1.1.3"
  },
  "lint-staged": {
    "src/**/*.{js}": ["eslint", "prettier --write", "git add"],
    "src/**/*.{css,less,json}": ["prettier --write", "git add"],
    "*.js": ["eslint", "prettier --write", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "prettier": {
    "trailingComma": "all"
  },
  "browserslist": {
    "development": ["last 1 chrome versions", "last 1 firefox versions"],
    "production": ["last 1 chrome versions", "last 1 firefox versions"]
  }
}
